{
  "name": "fluoride",
  "version": "0.0.1",
  "description": "Fluoride is a compile-time dependency injection framework for TypeScript.",
  "main": "build/index.js",
  "files": ["build/**/*.{js,js.map,d.ts,d.ts.map}", "LICENSE", "README.md"],
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha --require source-map-support/register --recursive build/**/*.spec.{js,jsx}",
    "coverage": "nyc check-coverage --lines 100 --statements 100 --branches 100 --functions 100"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hf/fluoride"
  },
  "keywords": [
    "typescript",
    "dependency",
    "injection",
    "compile-time",
    "inversion",
    "control",
    "ioc",
    "di",
    "object",
    "graph"
  ],
  "author": "Stojan Dimitrovski <sdimitrovski@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.14.2",
    "proxyquire": "^2.1.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.0"
  },
  "nyc": {
    "include": ["build/**/*.js"],
    "exclude": ["build/**/*.spec.js", "build/**/*.spec.jsx"],
    "extension": [".js", ".jsx"],
    "reporter": ["text-summary", "html", "lcovonly"],
    "sourceMap": true,
    "instrument": true
  }
}
